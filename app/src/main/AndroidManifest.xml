<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ryan.vibingplayer">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">


        <activity android:name=".albums_activity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".album_songs_activity"
            android:parentActivityName=".albums_activity"
            />
        <activity android:name=".currently_playing_activity"
            android:label="@string/currently_playing"
            android:parentActivityName="com.example.ryan.vibingplayer.album_songs_activity"
            >



        </activity>
    </application>

</manifest>