<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ryan.vibingplayer">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">


        <activity android:name=".AlbumsActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".AlbumSongsActivity"
            android:parentActivityName=".AlbumsActivity"
            />
        <activity android:name=".CurrentlyPlayingActivity"
            android:label="@string/currently_playing"
            android:parentActivityName="com.example.ryan.vibingplayer.AlbumSongsActivity"
            >



        </activity>
    </application>

</manifest>